		Scanner in=new Scanner(System.in);
		int n=in.nextInt();
		int Case_num=0;
		while(Case_num<n && n!=0){

			int a=in.nextInt();
			int b=in.nextInt();
			int c=in.nextInt();
			
			System.out.println("Case "+(++Case_num));
		}
	}

}
